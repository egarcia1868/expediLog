{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "ignorePatterns": ["models/index.js", "node_modules"],
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "no-console": "off",
        "prettier/prettier": "warn",
        "func-names": "off",
        "no-undef": "off",
        "no-restricted-syntax": "off",
        "no-unused-vars": "warn",
        "no-plusplus": "off",
        "no-restricted-globals": "off",
        "no-use-before-define": "off"
    }
}